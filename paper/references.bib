	note = {Number: 1},
	keywords = {strength, performance, body composition, 1RM, function},
	file = {Full Text PDF:C\:\\Users\\james\\Zotero\\storage\\XE2UF96F\\Fisher et al. - 2022 - The Role of Supervision in Resistance Training\; an.pdf:application/pdf},
}

@article{polito_moderators_2021,
	title = {Moderators of strength gains and hypertrophy in resistance training: {A} systematic review and meta-analysis},
	volume = {39},
	issn = {0264-0414},
	shorttitle = {Moderators of strength gains and hypertrophy in resistance training},
	url = {https://doi.org/10.1080/02640414.2021.1924978},
	doi = {10.1080/02640414.2021.1924978},
	abstract = {This meta-analysis investigated the role of resistance training (RT) moderators on strength and muscle mass gains in untrained young (YG) and older (OG) adults. Electronic databases were searched for randomised controlled trials simultaneously assessing muscle strength and mass. Effect sizes (ES) reflecting improvements in strength and muscle mass were found for all moderators in YG and OG (ES 0.25- to 1.72;p {\textless} 0.05), excepting muscle mass in YG after RT was performed with {\textless}3 sets/exercise. Strength gains (p {\textless} 0.001) were greater in non-periodised vs. periodised RT in YG (ES 1.72 vs. 1.05) and OG (1.40 vs. 0.74). ES in OG was greater (p {\textless} 0.04) when RT included non-failure vs. failure repetitions (1.35 vs. 0.96), 3 vs. {\textgreater}3 sets/exercise (1.30 vs. 0.90), ≥3 vs. {\textless}3 days/week (1.70 vs. 0.78), and ≥12 vs. {\textless}12 weeks (1.48 vs. 0.92). Amoderating effect of RT factors on muscle mass was not detected in YG, while greater ES was found in OG for RT with ≥3 vs. {\textless}3 days/week (0.50 vs. 0.25). Concluding, different combinations of RT factors improved strength and muscle mass in YG and OG. In OG, this was favoured by greater frequency and duration, although hampered by excessive volume.},
	number = {19},
	urldate = {2022-08-06},
	journal = {Journal of Sports Sciences},
	author = {Polito, Marcos D. and Papst, Rafael R. and Farinatti, Paulo},
	month = oct,
	year = {2021},
	pmid = {33977848},
	note = {Publisher: Routledge
\_eprint: https://doi.org/10.1080/02640414.2021.1924978},
	keywords = {Hypertrophy, muscle strength, resistance exercise, meta-analysis, training},
	pages = {2189--2198},
}

