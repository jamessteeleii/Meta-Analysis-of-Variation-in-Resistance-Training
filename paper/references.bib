	number = {1},
	urldate = {2022-10-05},
	journal = {American Journal of Hospice and Palliative Medicine®},
	author = {Fernandez, Carlos and Firdous, Shagufta and Jehangir, Waqas and Behm, Bertrand and Mehta, Zankhana and Berger, Andrea and Davis, Mellar},
	month = jan,
	year = {2020},
	note = {Publisher: SAGE Publications Inc},
	pages = {34--40},
	file = {SAGE PDF Full Text:C\:\\Users\\james\\Zotero\\storage\\GNJ7SSRX\\Fernandez et al. - 2020 - Cancer-Related Fatigue Perception of Effort or Ta.pdf:application/pdf},
}

