	keywords = {meta-analytic structural equation modeling, multi-level meta-analysis, open science, publication bias, Research synthesis},
	pages = {120--151},
	file = {Full Text:C\:\\Users\\james\\Zotero\\storage\\AGD29NME\\Hagger - 2022 - Meta-analysis.pdf:application/pdf},
