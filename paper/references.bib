	keywords = {meta-analysis, research synthesis, delta method, effect size estimator, large sample approximation, linearity of expectation, log ratio, small sample bias},
	pages = {2056--2063},
	file = {Full Text PDF:C\:\\Users\\james\\Zotero\\storage\\5WZ7XES3\\Lajeunesse - 2015 - Bias and correction for the log response ratio in .pdf:application/pdf;Snapshot:C\:\\Users\\james\\Zotero\\storage\\RUESG3WE\\14-2402.html:text/html},
}

@article{debQuantifyingEffectsAcute2018,
	title = {Quantifying the effects of acute hypoxic exposure on exercise performance and capacity: {A} systematic review and meta-regression},
